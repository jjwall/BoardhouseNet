  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.12",
    "@types/ws": "^6.0.3",
    "child_process": "^1.0.2",
    "copy-webpack-plugin": "^6.0.3",
    "ts-loader": "^6.2.0",
    "typescript": "^3.9.7",
